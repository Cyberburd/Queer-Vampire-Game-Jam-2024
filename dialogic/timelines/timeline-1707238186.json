		{
			"character": "character-1707139124.json",
			"event_id": "dialogic_001",
			"portrait": "Default",
			"text": "\"Things look like they are working out!\""
		},
		{
			"character": "character-1708005007.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"Sir, our reinforcements just arrived!\""
