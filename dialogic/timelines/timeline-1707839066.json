		{
			"character": "character-1708005167.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"Don't step any closer!\""
		},
		{
			"character": "character-1707139124.json",
			"event_id": "dialogic_001",
			"portrait": "Worried",
			"text": "\"I'm just trying to help...\""
		},
		{
			"character": "character-1708005167.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"How did you escape the prison cell?\""
		},
		{
			"character": "character-1707139124.json",
			"event_id": "dialogic_001",
			"portrait": "Default",
			"text": "\"I need some bandages and some water, please... I'm not trying to do any harm to anyone!\""
		},
		{
			"character": "character-1708005167.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"What happened? Tell me everything that happened!\""
		},
		{
			"character": "character-1707139124.json",
			"event_id": "dialogic_001",
			"portrait": "Angry",
			"text": "\"I need to help him now... Please! I will go back to the prison cell... Just please, get me the stuff!\""
		},
		{
			"character": "character-1708005167.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"Go to the cell... I'll bring you the stuff but we're all keeping watch now... You better go where Major Andersen really is...\""
		},
