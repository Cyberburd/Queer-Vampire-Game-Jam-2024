		{
			"character": "character-1707139124.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"General Von Dracula, the time has come!\""
		},
		{
			"character": "character-1707139107.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"Understood! Your strategy is as planned. I'm confident we can proceed!\""
		},
		{
			"character": "character-1707139107.json",
			"event_id": "dialogic_001",
			"portrait": "Happy",
			"text": "\"And thank you for letting me know directly...\nI know I can always count on you, Nathalian...\""
		},
		{
			"choice": "CALL EVERYONE!",
			"condition": "",
			"definition": "",
			"event_id": "dialogic_011",
			"value": ""
		},
		{
			"character": "character-1707139124.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"Call the Chief's cabinet to inform him\n that we are are going to battle, right now!\""
		},
		{
			"character": "character-1708005007.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": " \"Sir, we've reached our location!\nThe proceeding action is as planned.\""
		},
